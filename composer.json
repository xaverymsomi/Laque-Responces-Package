{
    "name": "vicent/laque-responses",
    "description": "A framework-agnostic PHP package that generates HTTP responses cleanly and consistently across ecosystems",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Vicent Msomi",
            "email": "msomivicent@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "psr/http-message": "^1.0 || ^2.0",
        "psr/http-factory": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "psr/container": "^2.0",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.14",
        "nyholm/psr7": "^1.5",
        "laminas/laminas-diactoros": "^2.24",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "LaqueResponses\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaqueResponses\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage",
        "phpstan": "phpstan analyse",
        "cs-check": "php-cs-fixer fix --dry-run --diff",
        "cs-fix": "php-cs-fixer fix"
    },
    "config": {
        "sort-packages": true
    }
}